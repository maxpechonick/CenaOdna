{
  "properties": [
    {
      "name": "security.jwt.tokenSigningKey",
      "type": "java.lang.String",
      "description": "token signing key."
    },
    {
      "name": "security.jwt.tokenIssuer",
      "type": "java.lang.String",
      "description": "token issuer."
    },
    {
      "name": "security.jwt.refreshTokenExpTime",
      "type": "java.lang.Integer",
      "description": "life time of refresh token."
    },
    {
      "name": "security.jwt.tokenExpirationTime",
      "type": "java.lang.Integer",
      "description": "life time of access token. it should be refreshed while this value is not expire in other case user should login again."
    }
  ]
}